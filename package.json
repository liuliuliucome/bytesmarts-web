{
  "name": "bytesmarts-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rimraf .contentlayer && contentlayer build && next dev -p 3103",
    "clean": "yarn playwright:clean && rimraf node_modules yarn.lock package-lock.json .contentlayer .next",
    "build:content": "contentlayer build",
    "build": "work:gen && next build",
    "preview": "next build && next start",
    "start": "next start",
    "lint": "next lint",
    "work:gen": "node ./work/bin/gen.js",
    "format": "prettier --write ./src",
    "pages:build": "contentlayer build && npx @cloudflare/next-on-pages",
    "playwright:install": "yarn playwright install",
    "playwright:clean": "rimraf playwright-report test-results"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@cloudflare/next-on-pages": "^1.8.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.201",
    "@vercel/analytics": "^1.0.0",
    "autoprefixer": "10.4.14",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "contentlayer": "^0.3.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "github-slugger": "^2.0.0",
    "kbar": "^0.1.0-beta.44",
    "lodash": "^4.17.21",
    "next": "13.4.1",
    "next-contentlayer": "^0.3.2",
    "next-themes": "^0.2.1",
    "postcss": "8.4.23",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "react": "18",
    "react-dom": "18",
    "shiki": "^0.14.5",
    "shiki-twoslash": "^3.1.2",
    "tailwindcss": "3.3.2",
    "unified": "^11.0.4",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "18.16.1",
    "@types/react": "18",
    "@types/react-dom": "18",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "sass": "^1.69.5",
    "typescript": "5.0.4"
  }
}
